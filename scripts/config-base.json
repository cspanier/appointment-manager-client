{
  "expected-versions": {
    "clang-format": ">=16.0.5,<17.0.0",
    "cmake": ">=3.27.2,<3.30.0",
    "ninja": ">=1.11.1,<2.0.0",
    "vcpkg": "=2023.12.12"
  },
  "build-log-path": null,
  "build-path-suffix": "",
