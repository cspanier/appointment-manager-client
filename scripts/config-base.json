  "definitions": {
    "USE_ADDRESS_SANITIZER": false,
    "USE_CPP20_MODULES": false,
    "USE_CPP_TOOLSET_VERSION_CHECK": true,
    "USE_DEVELOPMENT_VERSION": true,
    "USE_DIRTY_BUILD_CHECK": true,
    "USE_GOOGLE_CRASHPAD": true,
    "USE_TARGET_FOLDERS": false
